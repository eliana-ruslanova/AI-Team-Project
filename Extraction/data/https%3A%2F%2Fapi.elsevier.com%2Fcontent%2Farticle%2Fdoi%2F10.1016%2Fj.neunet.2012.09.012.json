rithm"}, {"@_fa": "true", "$": "Competitive learning"}, {"@_fa": "true", "$": "\n                     \n                        k\n                     -means clustering"}], "link": [{"@href": "https://api.elsevier.com/content/article/pii/S0893608012002420", "@rel": "self", "@_fa": "true"}, {"@href": "https://www.sciencedirect.com/science/article/pii/S0893608012002420", "@rel": "scidir", "@_fa": "true"}]}, "scopus-id": "84870469359", "scopus-eid": "2-s2.0-84870469359", "pubmed-id": "23137615", "link": {"@href": "https://api.elsevier.com/content/abstract/scopus_id/84870469359", "@rel": "abstract"}, "originalText": {"xocs:doc": {"xocs:meta": {"xocs:open-access": {"xocs:oa-article-status": {"@is-open-access": "0", "@is-open-archive": "0"}}, "xocs:available-online-date": {"@yyyymmdd": "20121001", "$": "2012-10-01"}}}}}